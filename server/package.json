  "version": "1.0.0",
  "main": "server.ts",
  "scripts": {
    "dev:server": "ts-node-dev --inspect --transpileOnly --ignore-watch node_modules src/server.ts",
    "start": "ts-node src/server.ts",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "run:migration": "ts-node-dev ./node_modules/typeorm/cli.js migrations:run",
    "revert:migration": "ts-node-dev ./node_modules/typeorm/cli.js migrations:revert",
    "create:migration": "ts-node-dev ./node_modules/typeorm/cli.js migrations:create -n"
  },
  "dependencies": {
    "aws-sdk": "^2.691.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "celebrate": "^12.0.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "date-fns": "^2.14.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nodemailer": "^6.4.2",
    "nodemailer-express-handlebars": "^3.1.0",
    "path": "^0.12.7",
    "pg": "^8.3.0",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^2.3.0",
    "typeorm": "^0.2.25",
    "typescript": "^3.9.3",
    "uuidv4": "^6.1.1"
